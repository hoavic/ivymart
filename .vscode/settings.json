{
    "liveSassCompile.settings.formats":[
        // This is Default.
        {
            "format": "compressed",
            "extensionName": ".css",
            "savePath": null
        },
/*         // You can add more
        {
            "format": "compressed",
            "extensionName": ".min.css",
            "savePath": null
        } */
    ],
    "liveSassCompile.settings.excludeList": [ 
        "**/node_modules/**",
        ".vscode/**" ,
        "wp-admin/**", 
        "wp-content/themes/hoango/scss/**"
    ],
    "autoprefixer.options": {
		"browsers": [
			"last 4 versions",
			"ie >= 9",
            "iOS >= 13",
            "Safari >= 12",
			"> 5%"
		]
    },
    "cSpell.ignoreWords": [
        "Giỏ",
        "aligncenter",
        "alignleft",
        "alignright",
        "bypostauthor",
        "callme",
        "callorder",
        "dashicons",
        "datetime",
        "hàng",
        "hoango",
        "ivymart",
        "neverending",
        "onsale",
        "searchform",
        "swiper",
        "tabindex",
        "woocommerce"
    ]
}